using namespace std;

int main()
{
float F,C;

cout<<"What is the temperature in Fahrenheit?";
cin>> F;
C = 5*(F-32)/9;

cout<<"Temperature in degrees Celsius is: " << C<<endl;

 if (C>=100){
cout<<"Water boil at this temperature (under typical conditions).";
 }

 else {
cout<<"Water does not boil at this temperature (under typical conditions).";
 }
    return 0;
}
